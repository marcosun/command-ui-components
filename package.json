    "test": "jest",
    "test:watch": "npm test -- --watch",
    "doc": "jsdoc src -r"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:marcosun/command-ui-components.git"
  },
  "keywords": [],
  "author": "Marco",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "globals": {
